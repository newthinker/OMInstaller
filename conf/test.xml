<?xml version="1.0" encoding="UTF-8"?>
<root OneMapHome="C:\20120915">
	<layout>
		<server name="token">
			<attr name="token_ip" desc="Token服务器ip:" value="127.0.0.2"/>
			<attr name="token_port" desc="Token服务器PORT:" value="8080" />
		</server>
		<server name="msg">
			<attr name="msg_ip" desc="JMS服务器ip:" value="127.0.0.2" />
			<attr name="msg_port" desc="JMS服务器PORT:" value="61616" />
		</server>
		<server name="agent">
			<attr name="agent_ip" desc="监控代理服务器IP:" value="127.0.0.2" />
			<attr name="agent_port" desc="监控代理服务器PORT:" value="8080" />
		</server>
	</layout>
	<filemapping>
		<container name="tomcat" path="Tomcat6.0.29\webapps">
			<module name="bufferanalyst">
				<server name="db">
					<file path="BufferAnalyst/WEB-INF/config/proxool.xml">
						<key template="jdbc:h2:tcp://[db_h2]/mem:geosharemdb;MODE=Oracle;DB_CLOSE_DELAY=-1;DB_CLOSE_ON_EXIT=false">/something-else-entirely/proxool[1]/driver-url</key>
						<key template="jdbc:oracle:thin:@[db_ip]:[db_port]:[db_sid]">/something-else-entirely/proxool[2]/driver-url</key>
						<key template="[manager_user]" attribute="value">/something-else-entirely/proxool[2]/driver-properties/property[@name='user']</key>
						<key template="[manager_pwd]" attribute="value">/something-else-entirely/proxool[2]/driver-properties/property[@name='password']</key>
					</file>			
				</server>
				<server name="gis">
					<file path="BufferAnalyst/WEB-INF/config/logback.xml">
						<key template="[TomcatAppPath]/BufferAnalyst/WEB-INF/logs" attribute="value">/configuration/property</key>
					</file>
				</server>
			</module>
		</container>
		<container name="services" path="services">
			<module name="h2commonmemdb">
				<server name="db">
					<file path="H2CommonMemDB/memdb-config.properties">
						<key template="jdbc:h2:tcp://[db_h2]/mem:geosharemdb;MODE=Oracle;DB_CLOSE_DELAY=-1;DB_CLOSE_ON_EXIT=false">memdb.url</key>
					</file>			
				</server>
			</module>
		</container>
	</filemapping>
</root>
