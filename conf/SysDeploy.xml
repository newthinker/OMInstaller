<?xml version="1.0" encoding="UTF-8"?>
<root>
	<nodes>
		<node name="node1">
			<!-- 属性 -->
			<attrs>
				<!-- 操作系统类型 -->
				<attr name="os"></attr>
				<!-- 操作系统架构32/64 -->
				<attr name="arch"></attr>
				<!-- 机器名 -->
				<attr name="macname"></attr>
				<!-- 域名 -->
				<attr name="domname"></attr>
				<!-- ip地址 -->
				<attr name="ip"></attr>
				<!-- 系统帐号 -->
				<attr name="user"></attr>
				<!-- 帐号密码 -->
				<attr name="pwd"></attr>
				<!-- OneMap安装路径 -->
				<attr name="omhome"></attr>
				<!-- 部署情况 [0:维持现状/1:安装/2:更新/3:卸载] -->
				<attr name="deploy"></attr>
		        <!-- 集群类型 [db/gis/main/agent/msg/token/web] -->
                <attr name="cluster_type"></attr>
			    <!-- 集群是否启用 [0:启用, 1:不启用] -->
			    <attr name="cluster_enabled"></attr>
			    <!-- 集群IP -->
			    <attr name="cluster_ip"></attr>
			</attrs>
			<!-- 安装的服务器 -->
			<servers>
				<server name="db"/>
				<server name="gis" />
				<server name="main" />
				<server name="agent" />
				<server name="msg" />
				<server name="token" />
				<server name="web" />
			</servers>
		</node>
	</nodes>
</root> 
